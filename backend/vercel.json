{
    "version": 2,
    "builds": [
    {
         "src": "server.js", "use": "@vercel/node",
        "use":"@vercel/node",
        "config":{
            "includeFiles":[
                "dist/**"
            ]
        }
        }
    
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/(.*)", "dest": "/src/$1" }  ]
}